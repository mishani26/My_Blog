@{
    Layout = null;
}

@model My_Blog.Models.ArticleModel

<div class="posts-wrap">
    <h3>Recent Posts</h3>
    <ul>
            <li>
                <p class="date">@DateTime.Now.Date.ToString("D")</p>
                <p class="post">
                    <a href="#">@Model.Post.Title</a>
                </p>
            </li>
    </ul>
</div>
